{
  "name": "form-builder",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "rimraf src/dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "rimraf src/dist && webpack --config webpack.config.aot.js --progress --profile --bail",
    "postbuild:aot": "gulp pre-deploy",
    "serve": "lite-server -c=bs-config.json",
    "serve:dev": "node server.js",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "serve:aot": "NODE_ENV=build node server.js",
    "start:dev": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "start": "node server.js",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "test": "karma start",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "test:coverage": "rimraf coverage && karma start karma-coverage.conf.js",
    "lint": "tslint './src/**/*.ts' -t verbose",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.json"
  },
  "publishConfig": {
    "registry": "http://usmlrs1278.arrow.com:8081/nexus/repository/npm-internal/"
  },
